<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:EquipmentRent.ViewModel"
                    xmlns:view="clr-namespace:EquipmentRent.MVVM.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:EquipmentsVM}">
        <view:Equipments/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:OrdersVM}">
        <view:Orders/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingsVM}">
        <view:Settings/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:UsersVM}">
        <view:Users/>
    </DataTemplate>
    
</ResourceDictionary>